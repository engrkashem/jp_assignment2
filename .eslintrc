{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
  ],
  "rules": {
    "@typescript-eslint/consistent-type-definitions": ["error", "type"],
    "no-unused-vars": "error",
    "no-unused-expressions": "error",
    "prefer-const": "warn",
    "no-console": "warn",
    "no-undef": "error",
  },
  "globals": {
    "process": "readonly",
  },
  "env": {
    "browser": true,
    "es2021": true,
  },
}
